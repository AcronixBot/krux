{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ES2022",
    "strict": false,
    "moduleResolution": "node",
    "declaration": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "dist/",
    "baseUrl": ".",
  },
  "include": [
    "src",
  ],
  "exclude": [
    "assets"
  ]
}